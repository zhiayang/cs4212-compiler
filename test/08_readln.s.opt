.global main_dummy
.type main_dummy, %function
main_dummy:
	@ spills:  <none>
	@ assigns:    '_t0' = a1;    '_c16' = a2;  'string' = a2;    '_c12' = v1
	@             '_c8' = v1;     'int' = v1;    'bool' = v2;     '_c4' = v3
	stmfd sp!, {v1, v2, v3, lr}
.main_dummy_entry:
	sub sp, sp, #4                          @ readln(int);; align adjustment (pre)
	bl __readln_int
	mov v1, a1
	bl __readln_bool
	mov v2, a1
	bl __readln_string
	mov a2, a1
	add sp, sp, #4                          @ align adjustment (post)
	ldr v3, =.string0                       @ _c4 = "your integer was:";
	stmfd sp!, {a2}                         @ println(_c4);; caller-save
	mov a2, v3
	add a2, a2, #4
	ldr a1, =.string1_raw
	bl printf(PLT)
	ldmfd sp, {a2}                          @ caller-restore
	mov a2, v1
	ldr a1, =.string2_raw
	bl printf(PLT)
	ldmfd sp!, {a2}                         @ caller-restore
	ldr v1, =.string3                       @ _c8 = "your boolean was:";
	stmfd sp!, {a2}                         @ println(_c8);; caller-save
	mov a2, v1
	add a2, a2, #4
	ldr a1, =.string1_raw
	bl printf(PLT)
	ldmfd sp, {a2}                          @ caller-restore
	movs a2, v2
	ldreq a2, =.string4_raw
	ldrne a2, =.string5_raw
	ldr a1, =.string1_raw
	bl printf(PLT)
	ldmfd sp!, {a2}                         @ caller-restore
	ldr v1, =.string6                       @ _c12 = "your string was:";
	stmfd sp!, {a2}                         @ println(_c12);; caller-save
	mov a2, v1
	add a2, a2, #4
	ldr a1, =.string1_raw
	bl printf(PLT)
	ldmfd sp!, {a2}                         @ caller-restore
	sub sp, sp, #4                          @ println(string);; align adjustment (pre)
	add a2, a2, #4
	ldr a1, =.string1_raw
	bl printf(PLT)
	add sp, sp, #4                          @ align adjustment (post)
	ldr a2, =.string7                       @ _c16 = "---";
	sub sp, sp, #4                          @ println(_c16);; align adjustment (pre)
	add a2, a2, #4
	ldr a1, =.string1_raw
	bl printf(PLT)
	mov a1, #1
	mov a2, #4
	bl calloc(PLT)
	mov a2, #69
	bl _J3Foo_5guessiE
	add sp, sp, #4                          @ align adjustment (post)
	b .main_dummy_exit                      @ return;
.main_dummy_exit:
	ldmfd sp!, {v1, v2, v3, pc}


.global _J3Foo_5guessiE
.type _J3Foo_5guessiE, %function
_J3Foo_5guessiE:
	@ spills:  <none>
	@ assigns: 'tries' = a2;   '_c39' = v1;    'num' = v1;   '_c26' = v2;   '_c29' = v2
	@           '_c32' = v2;    '_t0' = v2;    '_t1' = v2;    '_t2' = v2;    '_t4' = v2
	@              'x' = v3
	stmfd sp!, {v1, v2, v3, lr}
	mov v1, a2
._J3Foo_5guessiE_entry:
	mov v3, #0                              @ x = 0;
	mov a2, #0                              @ tries = 0;
	cmp v1, #0                              @ _t4 = 0 != num;
	bne ._J3Foo_5guessiE_L8
	b ._J3Foo_5guessiE_L9                   @ goto .L9;
._J3Foo_5guessiE_L1:
	cmp v3, v1                              @ _t0 = x != num;
	bne ._J3Foo_5guessiE_L8
	b ._J3Foo_5guessiE_L9                   @ goto .L9;
._J3Foo_5guessiE_L8:
	stmfd sp!, {a2}                         @ readln(x);; caller-save
	bl __readln_int
	mov v3, a1
	ldmfd sp!, {a2}                         @ caller-restore
	cmp v3, v1                              @ _t1 = x == num;
	beq ._J3Foo_5guessiE_L5
._J3Foo_5guessiE_L6:
	cmp v3, v1                              @ _t2 = x < num;
	blt ._J3Foo_5guessiE_L2
._J3Foo_5guessiE_L3:
	ldr v2, =.string8                       @ _c26 = "lower";
	stmfd sp!, {a2}                         @ println(_c26);; caller-save
	mov a2, v2
	add a2, a2, #4
	ldr a1, =.string1_raw
	bl printf(PLT)
	ldmfd sp!, {a2}                         @ caller-restore
	b ._J3Foo_5guessiE_L7                   @ goto .L7;
._J3Foo_5guessiE_L2:
	ldr v2, =.string9                       @ _c29 = "higher";
	stmfd sp!, {a2}                         @ println(_c29);; caller-save
	mov a2, v2
	add a2, a2, #4
	ldr a1, =.string1_raw
	bl printf(PLT)
	ldmfd sp!, {a2}                         @ caller-restore
	b ._J3Foo_5guessiE_L7                   @ goto .L7;
._J3Foo_5guessiE_L5:
	ldr v2, =.string10                      @ _c32 = "uwu";
	stmfd sp!, {a2}                         @ println(_c32);; caller-save
	mov a2, v2
	add a2, a2, #4
	ldr a1, =.string1_raw
	bl printf(PLT)
	ldmfd sp!, {a2}                         @ caller-restore
._J3Foo_5guessiE_L7:
	add a2, a2, #1                          @ tries = tries + 1;
	b ._J3Foo_5guessiE_L1                   @ goto .L1;
._J3Foo_5guessiE_L9:
	ldr v1, =.string11                      @ _c39 = "tries:";
	stmfd sp!, {a2}                         @ println(_c39);; caller-save
	mov a2, v1
	add a2, a2, #4
	ldr a1, =.string1_raw
	bl printf(PLT)
	ldmfd sp!, {a2}                         @ caller-restore
	sub sp, sp, #4                          @ println(tries);; align adjustment (pre)
	ldr a1, =.string2_raw
	bl printf(PLT)
	add sp, sp, #4                          @ align adjustment (post)
	b ._J3Foo_5guessiE_exit                 @ return;
._J3Foo_5guessiE_exit:
	ldmfd sp!, {v1, v2, v3, pc}



.global main
.type main, %function
main:
	str lr, [sp, #-4]!
	@ we need a 'this' argument for this guy, so just allocate nothing.
	sub sp, sp, #4
	mov a1, sp

	bl main_dummy

	add sp, sp, #4

	@ set the return code to 0
	mov a1, #0
	ldr pc, [sp], #4


.global __readln_int
.type __readln_int, %function
__readln_int:
	@ takes no args and returns the int
	stmfd sp!, {lr}
	sub sp, sp, #4          @ save some stack space (scanf wants a pointer)
	mov a2, sp              @ a2 is the pointer argument
	ldr a1, =.string12_raw
	bl scanf(PLT)
	cmp a1, #1              @ if scanf returned < 1...
	bge .__readln_int_ok
	mov a1, #0              @ just return 0.
	b .__readln_int_exit
.__readln_int_ok:
	ldr a1, [sp, #0]        @ load the value from stack
.__readln_int_exit:
	add sp, sp, #4          @ restore the stack
	ldmfd sp!, {pc}


.global __readln_bool
.type __readln_bool, %function
__readln_bool:
	@ takes no args and returns the bool
	@ accepts: '1_' (anything starting with '1', or 'T_'/'t_' (anything starting with 't')
	@ anything else is false.
	stmfd sp!, {lr}
	sub sp, sp, #12          @ space for the buffer
	mov a2, sp
	ldr a1, =.string13_raw
	bl scanf(PLT)
	cmp a1, #1              @ if scanf returned < 1, then it's probably eof?
	blt .__readln_bool_false
	ldrb a1, [sp, #0]       @ otherwise, load the first char
	cmp a1, #49             @ 49 = '1'
	beq .__readln_bool_true
	cmp a1, #84             @ 84 = 'T'
	beq .__readln_bool_true
	cmp a1, #116            @ 116 = 't'
	beq .__readln_bool_true
.__readln_bool_false:
	mov a1, #0
	b .__readln_bool_exit
.__readln_bool_true:
	mov a1, #1
.__readln_bool_exit:
	add sp, sp, #12
	ldmfd sp!, {pc}


.global __readln_string
.type __readln_string, %function
__readln_string:
	@ takes no args and returns the string
	stmfd sp!, {v1, lr}
	mov a1, #256            @ allocate 256 for the actual string
	add a1, a1, #5          @ plus 4 (len) + 1 (null term)
	mov a2, #1
	bl calloc(PLT)
	mov v1, a1              @ save it
	add a1, a1, #4          @ skip the length (will write later)
	mov a2, #256            @ buffer len
	ldr a3, =stdin
	ldr a3, [a3, #0]
	bl fgets(PLT)           @ 'a1' is now the string
	cmp a1, #0
	beq .__readln_string_bar
	bl strlen(PLT)          @ get the length
	cmp a1, #0
	beq .__readln_string_exit          @ don't do funny stuff (underflow)
	add a3, v1, a1
	add a3, a3, #4
	ldrb a2, [a3, #-1]      @ check the last char...
	cmp a2, #10
	beq .__readln_string_trim
	b .__readln_string_exit
.__readln_string_bar:
	mov a1, #0
	b .__readln_string_exit
.__readln_string_trim:                 @ get rid of the trailing newline
	mov a4, #0
	strb a4, [a3, #-1]
	sub a1, a1, #1
.__readln_string_exit:
	str a1, [v1, #0]        @ write the length
	mov a1, v1              @ return
	ldmfd sp!, {v1, pc}

